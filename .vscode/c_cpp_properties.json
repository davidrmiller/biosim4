{
    "configurations": [
        {
            "name": "Linux",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${workspaceFolder}/src/*"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "includePath": [
                "${default}",
                "/usr/include/SFML",
                "/usr/lib/x86_64-linux-gnu",
                "/usr/include",
                "/usr/include/opencv4",
                "${workspaceFolder}/src/include"
            ],
            "defines": [],
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}